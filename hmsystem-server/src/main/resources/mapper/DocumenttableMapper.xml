<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.hmsystem.server.mapper.DocumenttableMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="org.hmsystem.server.pojo.Documenttable">
        <id column="docnum" property="docnum" />
        <result column="docid" property="docid" />
        <result column="docstate" property="docstate" />
        <result column="doccreator" property="doccreator" />
        <result column="doccreatedate" property="doccreatedate" />
